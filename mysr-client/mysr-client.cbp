				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-m32" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-m32" />
					<Add library="../mysql/lib/mysr.a" />
