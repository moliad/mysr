			<Option virtualFolder="src/" />
		</Unit>
		<Unit filename="mysr-client.c">
			<Option compilerVar="CC" />
			<Option virtualFolder="src/" />
		</Unit>
		<Unit filename="mysr-usrpwd.h">
			<Option compilerVar="CC" />
			<Option virtualFolder="includes/" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
